.\" @(#)masterSI	40.10 /doccenter/unix_docs/src/doc/pomindex/386/s.masterSI 91/07/11 40.10 16:37:35
.\" Copyright 1991 AT&T
.\"  XX macro marks new index entry.
.de XX
.	br
.	ti 0
.	ne 2
.	vs 12
\s10\f1\\$1\0 \s8\fH\\$2\f1
..
.\"  header between letters of the alphabet
.de YY
.	sp 2
.	ne 4
\fH\s16\\$2\s0\f1
.	sp 0.5
..
.\"  markup surrounding book acronym
.ds B{
.ds }B
.\"  markup surrounding chapter number
.ds C{ " 
.ds }C :\ 
.vs 12
.ps 8
.CH "Index" I 
.pn 3
.H 1 "Index"
.2C
.in +2
